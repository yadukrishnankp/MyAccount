<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myaccount">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyAccount">
        <activity android:name=".Payment_Details_Activity" android:exported="true"></activity>
        <activity
            android:name=".Business_Activity"
            android:exported="true" />
        <activity android:name=".View_Business_Activity" />
        <activity android:name=".Forgetpassword_Activity" />
        <activity
            android:name=".Addbusiness_Activity"
            android:exported="true" />
        <activity
            android:name=".User_Home_Activity"
            android:exported="true" />
        <activity
            android:name=".All_userview_Activity"
            android:exported="true" />
        <activity android:name=".Admin_home_Activity" />
        <activity
            android:name=".Registration_Activity"
            android:exported="true" />
        <activity
            android:name=".Login_Activity"
            android:exported="true" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>